{
  "name": "synthetic-redux",
  "version": "1.0.3-0",
  "description": "Sets up Redux boilerplate: actions, reducers and redux-saga for most common redux use cases",
  "main": "src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "predeploy": "parcel build src/index.js --no-minify",
    "deploy": "np",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Midnight-Coder/synthetic-redux.git"
  },
  "keywords": [
    "synthetic-redux",
    "redux",
    "boilerplate",
    "webapp",
    "redux-saga"
  ],
  "author": "Sagar Jhobalia",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^27.1.1",
    "jest-coverage-badges": "^1.0.0",
    "np": "^7.5.0",
    "parcel-bundler": "^1.12.5"
  },
  "dependencies": {
    "redux-saga": "^1.1.3"
  }
}
