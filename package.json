{
  "name": "synthetic-redux",
  "version": "1.1.0",
  "description": "Sets up Redux boilerplate: actions, reducers and redux-saga for most common redux use cases",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "predeploy": "npm run build",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "watch": "microbundle-crl watch --no-compress --format modern,cjs",
    "deploy": "np",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Midnight-Coder/synthetic-redux.git"
  },
  "keywords": [
    "synthetic-redux",
    "redux",
    "boilerplate",
    "webapp",
    "redux-saga"
  ],
  "author": "Sagar Jhobalia",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "babel-jest": "^27.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^27.1.1",
    "jest-coverage-badges": "^1.0.0",
    "microbundle-crl": "^0.13.11",
    "np": "^7.5.0"
  },
  "dependencies": {
    "redux-saga": "^1.1.3"
  }
}
